@prefix : <http://www.example.org/timburtonontology/> .
@prefix ns1: <http://example.org/film/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix film: <http://www.example.org/timburtonontology/film/> .
@prefix onto: <http://www.example.org/timburtonontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix person: <http://www.example.org/timburtonontology/person/> .
@prefix genre_inst: <http://www.example.org/timburtonontology/genre/> .
@base <http://www.example.org/timburtonontology/> .

<http://www.example.org/timburtonontology/> rdf:type owl:Ontology ;
                                             owl:versionIRI <http://www.example.org/timburtonontology/1.0> ;
                                             rdfs:comment "This ontology was conjured to formally capture and categorize the wonderfully warped world of Tim Burton's films. It's a digital grimoire, meticulously mapping the recurring notions and peculiar threads that bind his filmography into a singular tapestry. Think of it as a tool for peering into Burton's cognitive signature—the unique mental blueprint that colors his every creation" .

#################################################################
#    Annotation properties
#################################################################

###  http://example.org/film/director
ns1:director rdf:type owl:AnnotationProperty .


###  http://example.org/film/releaseDate
ns1:releaseDate rdf:type owl:AnnotationProperty .


###  http://example.org/film/title
ns1:title rdf:type owl:AnnotationProperty .


###  http://www.example.org/timburtonontology/exhibitsAesthetic
onto:exhibitsAesthetic rdf:type owl:AnnotationProperty .


###  http://www.example.org/timburtonontology/hasDurationInMinutes
onto:hasDurationInMinutes rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.example.org/timburtonontology/PerformedBy
onto:PerformedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain onto:Character ;
                 rdfs:range onto:Actor ;
                 rdfs:comment "Connects a character role to the actor who portrays it." .


###  http://www.example.org/timburtonontology/belongsToGenre
onto:belongsToGenre rdf:type owl:ObjectProperty ,
                             owl:InverseFunctionalProperty ;
                    rdfs:domain onto:Film ;
                    rdfs:range onto:Genre ;
                    rdfs:comment "Links a film to its genre classification" .


###  http://www.example.org/timburtonontology/depictsCharacter
onto:depictsCharacter rdf:type owl:ObjectProperty ;
                      rdfs:domain onto:Film ;
                      rdfs:range onto:Character ;
                      rdfs:comment "Connect a Film to main Characters depicted in it." .


###  http://www.example.org/timburtonontology/directedBy
onto:directedBy rdf:type owl:ObjectProperty ;
                rdfs:domain onto:Film ;
                rdfs:range onto:Director ;
                rdfs:comment "Associates a film with its director." .


###  http://www.example.org/timburtonontology/embodiesArchetype
onto:embodiesArchetype rdf:type owl:ObjectProperty ;
                       rdfs:domain onto:Character ;
                       rdfs:range onto:Archetype ;
                       rdfs:comment "Links a character to the archetype it represents." .


###  http://www.example.org/timburtonontology/exhibitsAestethic
onto:exhibitsAestethic rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain onto:Film ;
                       rdfs:range onto:Aesthetic ;
                       rdfs:comment "Connects a film or element to a specific aesthetic style." .


###  http://www.example.org/timburtonontology/exploreArchetype
onto:exploreArchetype rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain onto:Film ;
                      rdfs:range onto:Archetype ;
                      rdfs:comment "Relates a film to archetypes it examines or features." .


###  http://www.example.org/timburtonontology/exploresTheme
onto:exploresTheme rdf:type owl:ObjectProperty ;
                   rdfs:domain onto:Film ;
                   rdfs:range onto:Theme ;
                   rdfs:comment "Associates a film with the themes it addresses." .


###  http://www.example.org/timburtonontology/featuresMotif
onto:featuresMotif rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain onto:Film ;
                   rdfs:range onto:NarrativeMotif ;
                   rdfs:comment "Connects a film or scene to recurring narrative motifs." .


###  http://www.example.org/timburtonontology/featuresObject
onto:featuresObject rdf:type owl:ObjectProperty ;
                    rdfs:domain onto:Film ;
                    rdfs:range onto:Elements ;
                    rdfs:comment "Links a film or scene to concrete objects present." .


###  http://www.example.org/timburtonontology/hasSharedRootConcept
onto:hasSharedRootConcept rdf:type owl:ObjectProperty ;
                          rdfs:domain owl:Thing ;
                          rdfs:range onto:SharedConcept ;
                          rdfs:comment "Connects an individual (e.g., an Element or Theme) to a broader shared conceptual root."@en ;
                          rdfs:label "Has Shared Root Concept"@en .


###  http://www.example.org/timburtonontology/involvesEvent
onto:involvesEvent rdf:type owl:ObjectProperty ;
                   rdfs:domain onto:Film ;
                   rdfs:range onto:Event ;
                   rdfs:comment "Connects a film or scene to significant events it includes" .


###  http://www.example.org/timburtonontology/isSetIn
onto:isSetIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain onto:Film ;
             rdfs:range onto:Location ;
             rdfs:comment "Links a film to the location where it takes place." .


###  http://www.example.org/timburtonontology/isThemeExpressedBy
onto:isThemeExpressedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain onto:Theme ;
                        rdfs:range onto:Character ;
                        rdfs:comment "Associates a theme with the character that manifest it." .


###  http://www.example.org/timburtonontology/producedBy
onto:producedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain onto:Film ;
                rdfs:range onto:Producer ;
                rdfs:comment "Associates a film with its producer." .


###  http://www.example.org/timburtonontology/starsIn
onto:starsIn rdf:type owl:ObjectProperty ;
             rdfs:domain onto:Actor ;
             rdfs:range onto:Film ;
             rdfs:comment "Links an actor to the film(s) in which they appear" .


###  http://www.example.org/timburtonontology/writtenBy
onto:writtenBy rdf:type owl:ObjectProperty ;
               rdfs:domain onto:Film ;
               rdfs:range onto:Screenwriter ;
               rdfs:comment "Connects a film to its screenwriter." .


#################################################################
#    Data properties
#################################################################

###  http://www.example.org/timburtonontology/hasDurationInMInutes
onto:hasDurationInMInutes rdf:type owl:DatatypeProperty ;
                          rdfs:domain onto:Film ;
                          rdfs:range xsd:integer ;
                          rdfs:comment "Specifies the length of a film in minutes." .


###  http://www.example.org/timburtonontology/hasName
onto:hasName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( onto:Actor
                                         onto:Director
                                         onto:Person
                                         onto:Producer
                                         onto:Screenwriter
                                       )
                         ] ;
             rdfs:range xsd:string ;
             rdfs:comment "Provides the name of a person or entity." .


###  http://www.example.org/timburtonontology/hasPlotSummary
onto:hasPlotSummary rdf:type owl:DatatypeProperty ;
                    rdfs:domain onto:Film ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Contains a textual summary of the film's narrative." .


###  http://www.example.org/timburtonontology/hasTitle
onto:hasTitle rdf:type owl:DatatypeProperty ;
              rdfs:domain onto:Film ;
              rdfs:range xsd:string ;
              rdfs:comment "Specifies the title of a film or creative work." .


###  http://www.example.org/timburtonontology/hasYear
onto:hasYear rdf:type owl:DatatypeProperty ;
             rdfs:domain onto:Film ;
             rdfs:range xsd:integer ;
             rdfs:comment "Indicates the release year or date associated with a film." .


#################################################################
#    Classes
#################################################################

###  http://www.example.org/timburtonontology/Actor
onto:Actor rdf:type owl:Class ;
           rdfs:subClassOf onto:Person ;
           rdfs:comment "A person who performs a character role in a film, contributing to the portrayal and interpretation of the narrative." .


###  http://www.example.org/timburtonontology/Aesthetic
onto:Aesthetic rdf:type owl:Class ;
               rdfs:subClassOf owl:Thing ;
               rdfs:comment "The visual style, design choices, and cinematographic techniques that establish the distinctive artistic identity of the films, encompassing patterns of color, composition, lighting, and overall visual presentation." .


###  http://www.example.org/timburtonontology/Archetype
onto:Archetype rdf:type owl:Class ;
               rdfs:subClassOf owl:Thing ;
               rdfs:comment "Recurring character types or symbolic figures that embody universal roles or psychological patterns, serving as narrative or thematic models within the storytelling." .


###  http://www.example.org/timburtonontology/Character
onto:Character rdf:type owl:Class ;
               rdfs:subClassOf onto:Person ;
               rdfs:comment "Individual entities or personas that have a central role in the narrative, actively participating in or influencing the story’s events." .


###  http://www.example.org/timburtonontology/Director
onto:Director rdf:type owl:Class ;
              rdfs:subClassOf onto:Person ;
              rdfs:comment "A person responsible for overseeing the artistic and dramatic aspects of a film, guiding the technical crew and actors to achieve a coherent vision" .


###  http://www.example.org/timburtonontology/Elements
onto:Elements rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              rdfs:comment "Tangible objects, creatures, or visual components that consistently appear in the films, including props, animals, patterns, and colors, as well as temporal markers, which are concrete rather than conceptual." .


###  http://www.example.org/timburtonontology/Event
onto:Event rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           rdfs:comment "Important occurrences or narrative actions that shape plot progression or character development, representing discrete happenings or sequences within the story." .


###  http://www.example.org/timburtonontology/Film
onto:Film rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          rdfs:comment "A creative work that presents a narrative through moving images and sound, typically produced for entertainment, artistic expression, or cultural purposes." .


###  http://www.example.org/timburtonontology/Genre
onto:Genre rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           rdfs:comment "The classification of the film based on narrative style, tone, and structural elements, reflecting the type or category of story told, beyond predefined genre labels." .


###  http://www.example.org/timburtonontology/Location
onto:Location rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              rdfs:comment "Places, settings, or environments that are narratively or atmospherically significant, contributing to the mood, tone, or unfolding of events within the film world." .


###  http://www.example.org/timburtonontology/NarrativeMotif
onto:NarrativeMotif rdf:type owl:Class ;
                    rdfs:subClassOf owl:Thing ;
                    rdfs:comment "Recurring structural patterns, storytelling techniques, or plot devices that appear consistently across the films, shaping the narrative framework and thematic expression." .


###  http://www.example.org/timburtonontology/Person
onto:Person rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            rdfs:comment "An individual human being who may participate in the creation, performance, or narrative of a film." .


###  http://www.example.org/timburtonontology/Producer
onto:Producer rdf:type owl:Class ;
              rdfs:subClassOf onto:Person ;
              rdfs:comment "A person who manages the production aspects of a film, including financing, logistics, scheduling, and overall project coordination." .


###  http://www.example.org/timburtonontology/Screenwriter
onto:Screenwriter rdf:type owl:Class ;
                  rdfs:subClassOf onto:Person ;
                  rdfs:comment "A person who creates the script or screenplay of a film, developing dialogue, scenes, and narrative structure." .


###  http://www.example.org/timburtonontology/SharedConcept
onto:SharedConcept rdf:type owl:Class ;
                   rdfs:subClassOf owl:Thing ;
                   rdfs:comment "A conceptual root or common theme shared by various elements or themes within Tim Burton's filmography, used for clustering related ideas."@en ;
                   rdfs:label "Shared Concept"@en .


###  http://www.example.org/timburtonontology/Theme
onto:Theme rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           rdfs:comment "Conceptual and interpretative ideas or underlying messages that recur throughout the films, representing abstract notions that influence narrative and character development. These themes are not tied to physical objects or specific entities." .


#################################################################
#    Individuals
#################################################################

###  http://www.example.org/timburtonontology/Absolem_AliceinWonderland
onto:Absolem_AliceinWonderland rdf:type owl:NamedIndividual ,
                                        onto:Character ;
                               onto:PerformedBy onto:Alan_Rickman ;
                               onto:embodiesArchetype onto:ArchetypeThe_Wise_Guide ;
                               rdfs:comment "The wise, smoking caterpillar who guides Alice to remember her destiny"@en ;
                               rdfs:label "Absolem the Caterpillar"@en .


###  http://www.example.org/timburtonontology/AdamMaitland_Beetlejuice
onto:AdamMaitland_Beetlejuice rdf:type owl:NamedIndividual ,
                                       onto:Character ;
                              onto:PerformedBy person:Alec_Baldwin ;
                              onto:embodiesArchetype onto:ArchetypeInnocent_Ghosts ,
                                                     onto:ArchetypeRecently_Deceased_Ghosts ;
                              rdfs:comment "Recently deceased husband who becomes a ghost trying to reclaim his home."@en ;
                              rdfs:label "Adam Maitland"@en .


###  http://www.example.org/timburtonontology/AestheticAmerican_Gothic_Folklore
onto:AestheticAmerican_Gothic_Folklore rdf:type owl:NamedIndividual ,
