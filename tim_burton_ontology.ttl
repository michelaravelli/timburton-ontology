                                                     onto:ElementsSkeletal_Figures ,
                                                     onto:ElementsSpiral_Hill ,
                                                     onto:ElementsTwisted_Holiday_Symbols ;
                                 onto:involvesEvent onto:EventCultural_Misunderstanding ,
                                                    onto:EventDisastrous_Delivery ,
                                                    onto:EventDiscovery_of_Other_World ,
                                                    onto:EventExistential_Crisis ,
